{
  "name": "github-action-skaffold",
  "version": "1.5.0",
  "private": true,
  "description": "Downloads Skaffold and Container Structure Tests utility",
  "main": "build/index.js",
  "readme": "README.md",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/hiberbee/github-action-skaffold/blob/latest/LICENSE"
    }
  ],
  "scripts": {
    "build": "ncc build build/index.js --source-map --out=lib",
    "clean": "rimraf build",
    "compile": "tsc",
    "fix": "prettier --write src/**/*.ts && eslint --fix src/**/*.ts",
    "release": "yarn clean && yarn compile && yarn build",
    "start": "cross-env RUNNER_TEMP=build node lib/index.js",
    "test": "jest",
    "postinstall": "is-ci || husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiberbee/github-action-skaffold.git"
  },
  "keywords": [
    "github-action",
    "ci-cd",
    "pipeline",
    "container-structure-test",
    "skaffold"
  ],
  "author": {
    "name": "Vlad Volkov",
    "email": "vlad@hiberbee.com",
    "url": "https://github.com/vladyslavvolkov"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/tool-cache": "^1.6.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "@zeit/ncc": "^0.22.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-github": "^4.1.2",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "is-ci": "^3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  }
}
